<Window x:Class="AimLabsFromWish.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Aim Labs from wish" Width="800" Height="600" Loaded="Window_Loaded">
    <Grid>
        <Canvas x:Name="canvas" Background="LightGray" MouseLeftButtonDown="Canvas_MouseLeftButtonDown">
            <TextBlock x:Name="scoreTextBlock" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10"
                       FontSize="20" FontWeight="Bold" Foreground="Black" Text="Score: 0"/>
        </Canvas>
    </Grid>
</Window>
